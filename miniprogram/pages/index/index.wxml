<!--index.wxml-->


<!-- <progress percent="40" stroke-width="12" />
<progress percent="60" color="pink" />
<progress percent="80" active /> -->


<view class="card">
    <view class="cardLine1">
      <image class="cardLine1Icon" src="../../images/basicprofile.jpeg"></image>
    </view>
    <text class="cardLine2">柒小仔</text>
    <view class="cardLine3">
      <text class="cardLine3T">会员经验</text>
      <text class="cardLine3V">1025/6000</text>
      <text class="cardLine3Arrow">></text>
    </view>
    <progress percent="20" />
    <view class="cardLine4">
      <image class="vipIcon" src="../../images/vip1.png"></image>
      <progress class="vipProgress" percent="60" activeColor="white" backgroundColor="#FF2B06" stroke-width="3" active="true" />
      <image class="vipIcon" src="../../images/vip2.png"></image>
      <progress class="vipProgress" percent="0" activeColor="white" backgroundColor="#FF2B06" stroke-width="3" />
      <image class="vipIcon" src="../../images/vip3.png"></image>
      <progress class="vipProgress" percent="0" activeColor="white" backgroundColor="#FF2B06" stroke-width="3" />
      <image class="vipIcon" src="../../images/vip4.png"></image>
    </view>
    <view class="cardLine5">
      <text class="vipText1">普通会员</text>
      <text class="vipText2">黄金会员</text>
      <text class="vipText3">铂金会员</text>
      <text class="vipText4">钻石会员</text>
    </view>
    <view class="cardLeftTriangle"></view>
    <view class="cardRightTriangle"></view>
</view>
<view class="cardTriangleAppendLine"></view>
<view class="container">

<progress percent="20" show-info />
<progress percent="40" stroke-width="12" />
<progress percent="60" color="pink" />
<progress percent="80" active />

  <!-- assets -->
  <view class="assets">
    <view class="assets-item-deposit" bindtap="unfinished">
      <view class="assets-item-content">
        <view class="assets-item-content-up">充值</view>
        <view class="assets-item-content-down">返现，立即提现</view>
      </view>
    </view>
    <view class="assets-item-ticket" bindtap="unfinished">
       <view class="assets-item-content">
        <view class="assets-item-content-up">卡券</view>
        <view class="assets-item-content-down">我的消费券</view>
       </view>
    </view>
    <view class="assets-item-remaining" bindtap="unfinished">
       <view class="assets-item-content">
        <view class="assets-item-content-up">余额</view>
        <view class="assets-item-content-down">即用即省</view>
       </view>
    </view>
  </view>

  <!-- signin -->
  <view class="signin" bindtap="unfinished">
    <view class="signin-u">
      <view class="signin-ul">
        <text class="signin-ul-text">签到专区</text>
      </view>
      <view class="signin-ur">
        <text class="signin-ur-text">签到好礼></text>
      </view>
    </view>
    <view class="signin-d">
      <view class="signin-d-view">
        <text class="signin-d-view-text">签到有惊喜，快去看看吧</text>
      </view>
    </view>
  </view>


  <!-- more 更多服务 -->

  <view class="more">
    <view class="more-header">
       <text class="more-header-text">更多服务</text>
    </view>
    <view class="more-list">
      <view class="more-list-line1">
        <!-- 订单管理 -->
        <view class="more-list-item-order" bindtap="unfinished">

          <view class="more-list-item-content">
            <view class="more-list-item-content-u">
              <image class="more-list-item-content-u-icon" src="../../images/order.png"></image>
            </view>
            <view class="more-list-item-content-d">
              <text class="more-list-item-content-d-text">订单管理</text>
            </view>
          </view>

        </view>

        <!-- 兑换中心 -->
        <view class="more-list-item-exchange" bindtap="unfinished">
          <view class="more-list-item-content">
            <view class="more-list-item-content-u">
              <image class="more-list-item-content-u-icon" src="../../images/exchange.png"></image>
            </view>
            <view class="more-list-item-content-d">
              <text class="more-list-item-content-d-text">兑换中心</text>
            </view>
          </view>
        </view>

        <!-- 会员积分 -->
        <view class="more-list-item-point" bindtap="unfinished">
          <view class="more-list-item-content">
            <view class="more-list-item-content-u">
              <image class="more-list-item-content-u-icon" src="../../images/point.png"></image>
            </view>
            <view class="more-list-item-content-d">
              <text class="more-list-item-content-d-text">会员积分</text>
            </view>
          </view>
        </view>

        <!-- 联系客服 -->
        <view class="more-list-item-customer-service" bindtap="unfinished">
          <view class="more-list-item-content">
            <view class="more-list-item-content-u">
              <image class="more-list-item-content-u-icon" src="../../images/customer-service.png"></image>
            </view>
            <view class="more-list-item-content-d">
              <text class="more-list-item-content-d-text">联系客服</text>
            </view>
          </view>
        </view>
        
      </view>

      <!-- line 2 -->
      <view class="more-list-line1">
        <!-- 意见反馈 -->
        <view class="more-list-item-order" bindtap="unfinished">

          <view class="more-list-item-content">
            <view class="more-list-item-content-u">
              <image class="more-list-item-content-u-icon" src="../../images/advice.png"></image>
            </view>
            <view class="more-list-item-content-d">
              <text class="more-list-item-content-d-text">意见反馈</text>
            </view>
          </view>

        </view>

        <!-- 邀请用户 -->
        <view class="more-list-item-exchange" bindtap="unfinished">
          <view class="more-list-item-content">
            <view class="more-list-item-content-u">
              <image class="more-list-item-content-u-icon" src="../../images/invite.png"></image>
            </view>
            <view class="more-list-item-content-d">
              <text class="more-list-item-content-d-text">邀请用户</text>
            </view>
          </view>
        </view>

        <!-- 商户入住 -->
        <view class="more-list-item-point" bindtap="bizreg">
          <view class="more-list-item-content">
            <view class="more-list-item-content-u">
              <image class="more-list-item-content-u-icon" src="../../images/bizreg.png"></image>
            </view>
            <view class="more-list-item-content-d">
              <text class="more-list-item-content-d-text">商户入住</text>
            </view>
          </view>
        </view>
        
      </view>
    </view>

  </view>

</view>
